// java_entity.stg

entityTemplate(name, fields) ::= <<
package nandi.project.generated;

import jakarta.persistence.*;
import java.util.*;

@Entity
public class <name> {

    <fields:field(); separator="\n">

}
>>

field(f) ::= <<
    <f.annotations: {a | <a>}; separator="\n">
    private <f.type><if(f.isArray)>[]<endif> <f.name>;
>>